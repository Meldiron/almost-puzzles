{
    "projectId": "almost-puzzles",
    "projectName": "Almost Puzzles",
    "settings": {
        "services": {
            "account": true,
            "avatars": true,
            "databases": true,
            "locale": true,
            "health": true,
            "storage": true,
            "teams": true,
            "users": true,
            "functions": true,
            "graphql": true,
            "messaging": true
        },
        "auth": {
            "methods": {
                "jwt": true,
                "phone": true,
                "invites": true,
                "anonymous": true,
                "email-otp": true,
                "magic-url": true,
                "email-password": true
            },
            "security": {
                "duration": 31536000,
                "limit": 0,
                "sessionsLimit": 10,
                "passwordHistory": 0,
                "passwordDictionary": false,
                "personalDataCheck": false,
                "sessionAlerts": false,
                "mockNumbers": []
            }
        }
    },
    "databases": [
        {
            "$id": "main",
            "name": "Main",
            "enabled": true
        }
    ],
    "collections": [
        {
            "$id": "finishes",
            "$permissions": [
                "create(\"users\")",
                "read(\"any\")"
            ],
            "databaseId": "main",
            "name": "Finishes",
            "enabled": true,
            "documentSecurity": true,
            "attributes": [
                {
                    "key": "userId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "gameId",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "mode",
                    "type": "string",
                    "required": true,
                    "array": false,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "levels",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 255,
                    "default": null
                },
                {
                    "key": "year",
                    "type": "integer",
                    "required": true,
                    "array": false,
                    "min": -9223372036854775808,
                    "max": 9223372036854775807,
                    "default": null
                },
                {
                    "key": "levelsSameDay",
                    "type": "string",
                    "required": false,
                    "array": true,
                    "size": 255,
                    "default": null
                }
            ],
            "indexes": []
        }
    ]
}